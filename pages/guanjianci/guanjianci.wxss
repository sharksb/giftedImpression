/**新增css zhb**/

.container {
  height: 100%;
}

.service-title {
  display: flex;
  margin-top: 50rpx;
  width: 90%;
  margin: 50rpx auto 30rpx;
  position: relative;
  font-weight: bold;
}

.services {
  display: flex;
  display: -webkit-flex;
  box-sizing: border-box;
  flex-wrap: wrap;
}

.services_box {
  width: 33.33333%;
}

.mylabel_box {
  flex: 1;
  width: 90%;
  margin: 0 auto;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  background-color: #f5f5f5;
  margin-bottom: 10rpx;
  border-radius: 10rpx;
  font-size: 26rpx;
  color: #757575;
  border: 1px solid rgba(0, 0, 0, 0);
}

checkbox {
  width: 100%;
  height: 100%;
}

.checkedbox {
  color: #39bfbf;
  border: 1px solid #39bfbf;
}

#keyword {
  width: 90%;
  padding-bottom: 15px;
  /* margin-bottom: 15px; */
  position: fixed;
  bottom: 0px;
  background-color: white;
}

#keywordQK {
  width: 100%;
  display: flex;

}

#keywordClear {
  width: 30%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #f5f5f5;
  color: #757575;
  font-size: 32rpx;
  border: 0px solid rgba(0, 0, 0, 0);
}

#keywordQD {
  width: 65%;
  height: 80rpx;
  line-height: 80rpx;
  background-color: #39bfbf;
  color: white;
  font-size: 32rpx;
}

/* pages/myWelfare/myWelfare.wxss */
.weboxlfare_box{
  width: 90%;
  margin: 0 auto;
  box-shadow: 0 6rpx 6rpx #6666;
  border-radius: 10px;
  margin-top: 40rpx;
}

.weboxlfare_box image{
  width: 100%;
  height: 300rpx;
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
}

.weboxlfarebox_content{
  display: flex;
  align-items: center;
}

.weboxlfareboxCon_title{
   color: #101010;
   font-size: 20px;
   width: 50%;
   text-align: center;
}

.weboxlfarebox_content button{
   width: 30%;
   height:80rpx;
   line-height: 80rpx;
   font-size: 28rpx;
   border-radius: 50rpx;
   margin: 20rpx auto;
}
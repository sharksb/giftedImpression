#reportTitle {
  display: flex;
  align-items: center;

  width:85%;
  margin: 0 auto;
    margin-top: 30px;
}

#reportButton {
  width: 198px;
  height: 46px;
  line-height: 46px;
  border-radius: 15px;
  background-color: rgba(19, 194, 194, 1);
  color: rgba(255, 255, 255, 1);
  font-size: 16px;
}

#attentionButton {
  width: 64px;
  height: 32px;
  line-height: 32px;
  border-radius: 18px;
  color: rgba(90, 89, 88, 1);
  font-size: 14px;
}

#reportNotice{
  width: 80%;
  margin: 0 auto;
  text-align: center;
  font-size: 16px;
  color:#9A9696;
  margin-top: 10px
}

.secondTitle{
  width: 85%;
  margin: 0 auto;
  margin-top: 20px;
  font-size: 16px;
  color:#404040;
}
.capabilityModel{
  position: relative;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  height: 250px;
}
ec-canvas {
  width: 100%;
  height: 100%;
}



